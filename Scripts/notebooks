        "class GoldVerifier:\n",
        "    def __init__(self, gold_db, id_to_label):\n",
        "        self.gold_db = gold_db\n",
        "        self.id_to_label = id_to_label\n",
        "        # ĞĞ¾Ğ²Ñ‹Ğµ, Ğ±Ğ¾Ğ»ĞµĞµ ÑÑ‚Ñ€Ğ¾Ğ³Ğ¸Ğµ Ğ¿Ğ¾Ñ€Ğ¾Ğ³Ğ¸\n",
        "        self.min_sim = 0.15      # Ğ‘Ñ‹Ğ» 0.12\n",
        "        self.high_conf = 0.22    # ĞŸĞ¾Ñ€Ğ¾Ğ³ Ğ±ĞµĞ·ÑƒÑĞ»Ğ¾Ğ²Ğ½Ğ¾Ğ³Ğ¾ Ğ´Ğ¾Ğ²ĞµÑ€Ğ¸Ñ\n",
        "        self.min_margin = 0.05   # Ğ‘Ñ‹Ğ» 0.03. ĞÑ‚Ñ€Ñ‹Ğ² Ğ´Ğ¾Ğ»Ğ¶ĞµĞ½ Ğ±Ñ‹Ñ‚ÑŒ Ğ·Ğ°Ğ¼ĞµÑ‚Ğ½ĞµĞµ.\n",
        "\n",
        "    def predict(self, embedding):\n",
        "        scores = []\n",
        "        for label_id, gold_vec in self.gold_db.items():\n",
        "            sim = np.dot(embedding, gold_vec)\n",
        "            scores.append((sim, label_id))\n",
        "\n",
        "        scores.sort(key=lambda x: x[0], reverse=True)\n",
